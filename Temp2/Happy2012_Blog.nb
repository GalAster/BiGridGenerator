  3.537103014944989*^9}, {3.537183999744722*^9, 3.537184018058695*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "m"}], 
      RowBox[{"p", "-", "1"}]], 
     RowBox[{"-", "i"}]}], ")"}], "+", 
   RowBox[{"p", " ", 
    RowBox[{"(", 
     RowBox[{"p", "+", "1"}], ")"}]}], "+", 
   RowBox[{"(", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", 
       RowBox[{"p", "+", "2"}]}], "n"], "i"}], ")"}]}], " ", "//", " ", 
  "Simplify"}]], "Input",
