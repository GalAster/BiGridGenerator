   RowBox[{"PageFooters", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"None", ",", "None", ",", "None"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Cell", "[", 
         RowBox[{
          RowBox[{"TextData", "[", 
           RowBox[{
           "{", "\"\<\[Copyright] 2008 [[***YOUR NAME OR COMPANY HERE***]]. \
All rights reserved.\>\"", "}"}], "]"}], ",", "\"\<PageFooter\>\""}], "]"}], 
        ",", "None", ",", "None"}], "}"}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"PageHeaders", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"None", ",", "None", ",", "None"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"None", ",", "None", ",", 
        RowBox[{"Cell", "[", 
         RowBox[{
          RowBox[{"TextData", "[", 
           RowBox[{"{", 
