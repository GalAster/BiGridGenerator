Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"m", "(", "i", ")"}], "=", 
    RowBox[{
     RowBox[{"2", "i"}], "-", "1"}]}], TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"n", "(", "j", ")"}], "=", 
    RowBox[{
     RowBox[{"2", "j"}], "+", "1"}]}], TraditionalForm]], "InlineMath"],
 ". Each blue curve is given parametrically by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "\[LongRightArrow]"}], TraditionalForm]], "InlineMath"],
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      RowBox[{"m", "(", "i", ")"}], " ", 
      RowBox[{"n", "(", "j", ")"}]}], ",", " ", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"m", "(", "i", ")"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{"n", "(", "j", ")"}], "2"]}]}], ")"}], TraditionalForm]], 
  "InlineMath"],
 " for fixed ",
 Cell[BoxData[
  FormBox["j", TraditionalForm]], "InlineMath"],
 "; different values of ",
 Cell[BoxData[
  FormBox["j", TraditionalForm]], "InlineMath"],
 " give different blue curves. The blue curves shown correspond to integer \
choices of ",
 Cell[BoxData[
  FormBox["j", TraditionalForm]], "InlineMath"],
 ". Similarly, each red curve is given parametrically by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"j", "\[LongRightArrow]", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"m", "(", "i", ")"}], 
       RowBox[{"n", "(", "j", ")"}]}], ",", " ", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"n", "(", "j", ")"}], "2"], "-", 
       SuperscriptBox[
        RowBox[{"m", "(", "i", ")"}], "2"]}]}], ")"}]}], TraditionalForm]], 
  "InlineMath"],
 " for fixed ",
 Cell[BoxData[
  FormBox["i", TraditionalForm]], "InlineMath"],
 "; different values of ",
 Cell[BoxData[
  FormBox["i", TraditionalForm]], "InlineMath"],
 " give different red curves. The red curves shown correspond to integer \
choices of ",
 Cell[BoxData[
  FormBox["i", TraditionalForm]], "InlineMath"],
 ". "
