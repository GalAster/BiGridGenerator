Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"SeedRandom", "[", "1", "]"}], ";", 
    RowBox[{"DynamicModule", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"rn", "=", 
         RowBox[{"Partition", "[", 
          RowBox[{
           RowBox[{"RandomSample", "[", 
            RowBox[{"Range", "[", 
             RowBox[{"0", ",", 
              RowBox[{
               RowBox[{"-", "1"}], "+", 
               RowBox[{"r", " ", "r"}]}]}], "]"}], "]"}], ",", "r"}], "]"}]}],
         ",", 
        RowBox[{"rr", "=", 
         RowBox[{"Array", "[", 
          RowBox[{"List", ",", 
           RowBox[{"{", 
            RowBox[{"r", ",", "r"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", "0"}], "}"}]}], "]"}]}]}], "}"}], ",", 
      RowBox[{"Dynamic", "@", 
       RowBox[{"ClickPane", "[", 
        RowBox[{
         RowBox[{"Dynamic", "@", 
          RowBox[{"Graphics", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"MapThread", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"If", "[", 
                   RowBox[{
                    RowBox[{"#1", "\[Equal]", "0"}], ",", 
                    RowBox[{"Opacity", "[", "0", "]"}], ",", 
                    RowBox[{"Hue", "[", 
                    RowBox[{
                    RowBox[{"1", "-", 
                    RowBox[{"#1", "/", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"-", "1"}], "+", 
                    RowBox[{"r", " ", "r"}]}], ")"}]}]}], ",", "1", ",", "1", 
                    ",", "1"}], "]"}]}], "]"}], ",", 
                  RowBox[{"Disk", "[", 
                   RowBox[{"#2", ",", ".5"}], "]"}], ",", "White", ",", 
                  RowBox[{"Text", "[", 
                   RowBox[{
                    RowBox[{"Style", "[", 
                    RowBox[{"#1", ",", 
                    RowBox[{"FontSize", "\[Rule]", 
                    RowBox[{"200", "/", "r"}]}]}], "]"}], ",", "#2"}], 
                   "]"}]}], "}"}], "&"}], ",", 
               RowBox[{"{", 
                RowBox[{"rn", ",", "rr"}], "}"}], ",", "2"}], "]"}], "}"}], 
            ",", 
            RowBox[{"Background", "\[Rule]", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"Flatten", "[", 
                 RowBox[{"Reverse", "@", 
                  RowBox[{"Transpose", "@", "rn"}]}], "]"}], "\[Equal]", 
                RowBox[{"Range", "[", 
                 RowBox[{"0", ",", 
                  RowBox[{
                   RowBox[{"-", "1"}], "+", 
                   RowBox[{"r", " ", "r"}]}]}], "]"}]}], ",", "White", ",", 
               "Black"}], "]"}]}], ",", 
            RowBox[{"ImagePadding", "\[Rule]", "None"}], ",", 
            RowBox[{"ImageMargins", "\[Rule]", "None"}], ",", 
            RowBox[{"PlotRange", "\[Rule]", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{
                 RowBox[{
                  RowBox[{"-", "1"}], "/", "2"}], ",", 
                 RowBox[{"r", "-", 
                  RowBox[{"1", "/", "2"}]}]}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{
                  RowBox[{"-", "1"}], "/", "2"}], ",", 
                 RowBox[{"r", "-", 
                  RowBox[{"1", "/", "2"}]}]}], "}"}]}], "}"}]}], ",", 
            RowBox[{"PlotRangePadding", "\[Rule]", "None"}]}], "]"}]}], ",", 
         RowBox[{"Function", "[", 
          RowBox[{
           RowBox[{"{", "t", "}"}], ",", 
           RowBox[{"With", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"zp", "=", 
                RowBox[{
                 RowBox[{"Position", "[", 
                  RowBox[{"rn", ",", "0"}], "]"}], "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}], ",", 
               RowBox[{"hp", "=", 
                RowBox[{"(", 
                 RowBox[{"1", "+", 
                  RowBox[{"Round", "[", "t", "]"}]}], ")"}]}]}], "}"}], ",", 
             RowBox[{"With", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"hpv", "=", 
                 RowBox[{"rn", "[", 
                  RowBox[{"[", 
                   RowBox[{"Sequence", "@@", "hp"}], "]"}], "]"}]}], "}"}], 
               ",", 
               RowBox[{"If", "[", 
                RowBox[{
                 RowBox[{
                  RowBox[{"Total", "[", 
                   RowBox[{"Abs", "[", 
                    RowBox[{"zp", "-", "hp"}], "]"}], "]"}], "\[Equal]", 
                  "1"}], ",", 
                 RowBox[{"rn", "=", 
                  RowBox[{"ReplacePart", "[", 
                   RowBox[{"rn", ",", 
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"hp", "\[Rule]", "0"}], ",", 
                    RowBox[{"zp", "\[Rule]", "hpv"}]}], "}"}]}], "]"}]}]}], 
                "]"}]}], "]"}]}], "]"}]}], "]"}]}], "]"}]}]}], "]"}]}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"r", ",", "3", ",", "\"\<rank\>\""}], "}"}], ",", "3", ",", "6",
      ",", "1"}], "}"}]}], "]"}]], "Input",
