Cell[BoxData[
 RowBox[{
  RowBox[{"RandomInkBlot", "[", 
   RowBox[{"mu_", ",", "sigma_", ",", "seed_", ",", "resolution_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"pStart", "=", 
       RowBox[{"Range", "[", 
        RowBox[{"0", ",", "1", ",", 
         RowBox[{"1", "/", "resolution"}]}], "]"}]}], ",", "pX", ",", "pY"}], 
     "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"BlockRandom", "[", 
      RowBox[{
       RowBox[{"SeedRandom", "[", "seed", "]"}], ";", 
       RowBox[{"pX", "=", 
        RowBox[{"FoldList", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"#1", "+", 
             RowBox[{"RandomReal", "[", 
              RowBox[{"NormalDistribution", "[", 
               RowBox[{
                RowBox[{"mu", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], ",", 
                RowBox[{"sigma", "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "]"}]}], ")"}], 
           "&"}], ",", "0", ",", 
          RowBox[{"Rest", "@", "pStart"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"pY", "=", 
        RowBox[{"FoldList", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"#1", "+", 
             RowBox[{"RandomReal", "[", 
              RowBox[{"NormalDistribution", "[", 
               RowBox[{
                RowBox[{"mu", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], ",", 
                RowBox[{"sigma", "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "]"}]}], ")"}], 
           "&"}], ",", "0", ",", 
          RowBox[{"Rest", "@", "pX"}]}], "]"}]}], ";"}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Graphics", "[", 
      RowBox[{
       RowBox[{"Polygon", "[", 
        RowBox[{"Join", "[", 
         RowBox[{
          RowBox[{"Transpose", "[", 
           RowBox[{"{", 
            RowBox[{"pX", ",", "pY"}], "}"}], "]"}], ",", 
          RowBox[{"Reverse", "@", 
           RowBox[{"Transpose", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "pX"}], ",", "pY"}], "}"}], "]"}]}]}], "]"}], 
        "]"}], ",", 
       RowBox[{"AspectRatio", "\[Rule]", "1"}], ",", 
       RowBox[{"ImageSize", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"400", ",", "400"}], "}"}]}]}], "]"}]}]}], "]"}]}]], "Input",
